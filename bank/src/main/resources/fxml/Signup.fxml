<AnchorPane fx:id="dashpane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: black; -fx-background-radius: 40px;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Bank.FXMLController">
