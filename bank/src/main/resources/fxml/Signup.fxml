      <JFXTextField fx:id="email" alignment="CENTER" focusColor="WHITE" labelFloat="true" layoutX="37.0" layoutY="392.0" prefHeight="33.0" prefWidth="299.0" promptText="Email" style="-fx-background-color: white; -fx-background-radius: 10px; -jfx-focus-color: white;" styleClass="color" stylesheets="@../styles/Styles.css" unFocusColor="RED">
